<%= @scheduler['cron'] %>
<%= form_tag(schedulers_path, method: "put") do %>
    <div class="form-group">
      <label class="col-lg-3 control-label" for="name">Cron job *</label>
      <div class="col-lg-7">
        <input class="form-control required" placeholder="User Name" id="user_name" name="scheduler[cron]"
               type="text" value="<%= @scheduler['cron'] %>">
      </div>
    </div>
    <input type="hidden" name="scheduler[queue]" >
      <div class="form-group">
        <div class="col-lg-12 text-center">
          <%= link_to "Cancel" %>
          <%= button_tag "Submit" %>
        </div>
      </div>
<% end %>